{
  "compilerOptions": {
    "outDir": "./lib",
    "baseUrl": "./src",
    "module": "ES6",
    "moduleResolution": "node",
    "target": "ES6",
    "declaration": true,
    "jsx": "react",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "removeComments": false,
    "experimentalDecorators": true,
    "sourceMap": true,
    "skipDefaultLibCheck": false,
    "esModuleInterop": true,
    "strict": true,
    "lib": ["es2015.iterable", "es2015.generator", "es6", "dom"]
  }
}
